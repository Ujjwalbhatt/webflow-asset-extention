{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "ext:build": "EXT=true nuxt generate",
    "ext:dev": "EXT=true nodemon --watch src --ext ts,vue --exec \"npm run ext:build && npm run ext:serve\"",
    "ext:serve": "webflow extension serve"
  },
  "dependencies": {
    "nuxt": "^3.13.0",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@webflow/designer-extension-typings": "^2.0.2",
    "@webflow/webflow-cli": "^1.6.12",
    "concurrently": "^9.0.1",
    "nodemon": "^3.1.7",
    "openai": "^4.67.2"
  }
}
